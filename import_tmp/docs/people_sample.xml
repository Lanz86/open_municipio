<?xml version="1.0" encoding="UTF-8"?>
<!-- 
Soluzione temporanea per il file people.xml db atti formali.

E' un elenco di persone, con nome e cognome e il codice interno del db atti formali.
L'attributo charge specifica l'incarico. 
I valori possibili:
 - mayor (sindaco)
 - firstdeputy (vice sindaco)
 - deputy (assessore)
 - counselor (consigliere)
-->
<om:People xmlns:xlink="http://www.w3.org/1999/xlink"
 xmlns:om="http://www.openmunicipio.it"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.openmunicipio.it OpenMunicipioDataImport.xsd">

    <om:Person id="IVLE" first_name="Ivo" last_name="Lenardon" sex="m" charge="mayor"/>
    <om:Person id="SACO" first_name="Salvatore" last_name="Cortis" charge="firstdeputy"/>
    <om:Person id="GIGO" first_name="Giacomo" last_name="Gonfiotti" sex="m" charge="deputy"/>

    <!-- 
     Una persona con doppio incarico (assessore e consigliere) Ã¨ specificata 
     due volte, con codice id differente
    -->
    <om:Person id="GIBI" first_name="Giovanna" last_name="Biancucci" sex="f" charge="deputy"/>
    <om:Person id="GIBC" first_name="Giovanna" last_name="Biancucci" sex="f" charge="counselor"/>
</om:People>
